# Environment Variables Template
# Copy this file to .env and fill in your actual values
# Never commit the actual .env file to git!

# You can use either format:
# 1. Simple format: KEY=value
# 2. Export format: export KEY=value

# AWS Configuration
export AWS_ACCESS_KEY_ID=your_aws_access_key_here
export AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
export AWS_DEFAULT_REGION=us-west-2
export AWS_PROFILE=default

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_username
DB_PASSWORD="your_password_with_spaces"

# API Keys
export OPENAI_API_KEY=your_openai_api_key_here
export GITHUB_TOKEN=your_github_token_here
export DOCKER_HUB_TOKEN=your_docker_hub_token_here

# Application Settings
NODE_ENV=development
PORT=3000
DEBUG=false

# PATH additions (use export for PATH modifications)
export PATH="/usr/local/custom/bin:$PATH"

# Complex values with quotes
export CUSTOM_CONFIG='{"key": "value", "nested": {"item": "data"}}'

# Add your own environment variables below:
# CUSTOM_VAR=your_value_here
# export ANOTHER_VAR="value with spaces"
